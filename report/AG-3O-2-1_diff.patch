+        print(f"\nReceived {sig_name}, requesting graceful shutdown...")
+        stop_controller.request_stop(sig_name)
+    
+    signal.signal(signal.SIGINT, _handler)
+    
+    if hasattr(signal, 'SIGTERM'):
+        signal.signal(signal.SIGTERM, _handler)
